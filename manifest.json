{
  "manifest_version": 1,
  "type": "WebApp",
  "meta": {
    "name": "Pr√©dictions LoL Automatiques",
    "version": "1.2.1",
    "minimum-overwolf-version": "0.165.0",
    "author": "Sididi",
    "icon": "icons/tds.png",
    "icon_gray": "icons/tds.png",
    "launcher_icon": "icons/tds.ico",
    "window_icon": "icons/tds.png",
    "description": "League of Legends automatic Twitch prediction"
  },
  "permissions": [
    "Extensions",
    "FileSystem",
    "Tray"
  ],
  "data": {
    "game_targeting": {
      "type": "dedicated",
      "game_ids": [
        10902
      ]
    },
    "start_window": "background",
    "externally_connectable": {
      "matches": [
        "http://localhost",
        "https://oauth.sididi.tv",
        "http://*.twitch.tv",
        "https://*.twitch.tv"
      ]
    },
    "windows": {
      "background": {
        "file": "background.html",
        "is_background_page": true,
        "background_optimization": false,
        "allow_local_file_access": true
      },
      "main": {
        "file": "main.html",
        "transparent": false,
        "resizable": true,
        "show_in_taskbar": true,
        "keep_window_location": true,
        "desktop_only": true,
        "show_maximize": true,
        "override_on_update": true,
        "use_os_windowing": true,
        "size": {
          "width": 768,
          "height": 432
        },
        "min_size": {
          "width": 640,
          "height": 360
        },
        "start_position": {
          "top": 300,
          "left": 300
        }
      }
    },
    "game_events": [10902],
    "launch_events": [
      {
        "event": "GameLaunch",
        "event_data": {
          "game_ids": [
            10902
          ]
        },
        "start_minimized": true
      }
    ]
  }
}
